{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": ["feature", "feat", "add", "添加"]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": ["fix","修复"]
        },
        {
            "title": "## 🧪 Docs",
            "labels": ["README"]
        },
        {
            "title": "## 🧪 Update",
            "labels": ["update"]
        }
    ],
    "label_extractor": [
        {
            "pattern": ".*((?i)feature|(?i)feat|(?i)add|添加).*",
            "target": "$1",
            "on_property": "title"
        },
        {
            "pattern": ".*((?i)fix|修复).*",
            "target": "$1",
            "on_property": "title"
        },
        {
            "pattern": ".*(?i)README.*",
            "target": "$1",
            "on_property": "title"
        },
        {
            "pattern": ".*(?i)update.*",
            "target": "$1",
            "on_property": "title"
        }
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}",
    "pr_template": "- ${{TITLE}} - PR: #${{NUMBER}} by@{{ASSIGNEES}}",
    "empty_template": "- no changes",
    "max_pull_requests": 1000,
    "max_back_track_time_days": 1000
}

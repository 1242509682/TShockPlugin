{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": ["Feat", "Add", "feature", "feat", "add", "添加"]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": ["fix","Fix", "修复"]
        },
        {
            "title": "## 🧪 Docs",
            "labels": ["README"]
        },
        {
            "title": "## 🧪 Update",
            "labels": ["update","Update"]
        }
    ],
    "label_extractor": [
        {
            "pattern": ".*(feature|feat|add|添加|Feat|Add).*",
            "target": "$1",
            "on_property": "title"
        },
        {
            "pattern": ".*(fix|修复|Fix).*",
            "target": "$1",
            "on_property": "title"
        },
        {
            "pattern": ".*README.*",
            "target": "$1",
            "on_property": "title"
        },
        {
            "pattern": ".*(update|Update).*",
            "target": "$1",
            "on_property": "title"
        }
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}",
    "pr_template": "- ${{TITLE}} - PR: #${{NUMBER}} by@#{{DAYS_SINCE}}",
    "empty_template": "#{{OWNER}}\n#{{REPO}}\n#{{FROM_TAG}}\n#{{TO_TAG}}",
    "max_pull_requests": 1000,
    "max_back_track_time_days": 1000
}
